{
	"name": "imskm/axis-bank-api",
	"description": "A library to make axis bank api implementation easier",
	"keywords": ["axis-bank", "bank", "api", "axis bank api"],
	"authors": [
		{
			"name": "Shek Muktar",
			"email": "skm_im07@live.com"
		}
	],
	"require": {
		"php": ">=7.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9"
	},
	"autoload": {
		"psr-4": {
			"AxisBankApi\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"AxisBankApi\\Tests\\": "tests/"
		}
	},
	"config": {
		"sort-packages": true
	},
	"prefer-stable": true
}
